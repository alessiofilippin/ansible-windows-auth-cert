[windowsvms]
10.0.0.11

[windowsvms:vars]
ansible_connection=winrm
ansible_winrm_cert_pem=/home/administrator/.ssh/windowscerts/cert.pem  ## This is the SelfSigned Cert
ansible_winrm_cert_key_pem=/home/administrator/.ssh/windowscerts/cert_key.pem ## This is the SelfSigned Cert
ansible_winrm_transport=certificate
ansible_winrm_scheme=https
ansible_winrm_server_cert_validation=ignore
ansible_python_interpreter= /usr/bin/python3
